{
  "name": "warene",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "executor": "npm run build && node --enable-source-maps ./dist/bin/executor",
    "executor:dev": "nodemon --config nodemon-executor.json",
    "init": "npm run build && node --enable-source-maps ./dist/bin/init",
    "init:dev": "npm run build && node --inspect ./dist/bin/init",
    "server": "npm run build && node --enable-source-maps ./dist/bin/serve.js",
    "server:dev": "nodemon --config nodemon.json",
    "ts-node": "ts-node",
    "tsc": "tsc",
    "worker": "npm run build && node --enable-source-maps ./dist/bin/worker",
    "worker:dev": "nodemon  --config nodemon-worker.json"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connect-livereload": "^0.6.1",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "~1.4.4",
    "cryptr": "^6.0.3",
    "csv-parser": "^3.0.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "morgan": "~1.9.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "playwright": "^1.21.1",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.1",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bootstrap": "^5.1.10",
    "@types/connect-livereload": "^0.6.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cryptr": "^4.0.1",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/http-errors": "^1.8.2",
    "@types/livereload": "^0.9.2",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.25",
    "@types/validator": "^13.7.2",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
