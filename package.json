{
  "name": "warene",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "executor": "npm run build && node ./dist/bin/executor",
    "executor:dev": "nodemon --config nodemon-executor.json",
    "init": "npm run build && node ./dist/bin/init",
    "init:dev": "npm run build && node --inspect ./dist/bin/init",
    "server": "npm run build && node ./dist/bin/serve.js",
    "server:dev": "nodemon --config nodemon.json",
    "ts-node": "ts-node",
    "tsc": "tsc",
    "worker": "npm run build && node ./dist/bin/worker",
    "worker:dev": "nodemon  --config nodemon-worker.json"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "better-sqlite3-session-store": "0.0.3",
    "connect-livereload": "^0.6.1",
    "connect-session-sequelize": "^7.1.3",
    "cookie-parser": "~1.4.4",
    "csv-parser": "^3.0.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "morgan": "~1.9.1",
    "playwright": "^1.21.1",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "sequelize-typescript": "^2.1.3",
    "socket.io": "^4.5.0",
    "sqlite3": "^5.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bootstrap": "^5.1.10",
    "@types/connect-livereload": "^0.6.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/http-errors": "^1.8.2",
    "@types/livereload": "^0.9.2",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.25",
    "@types/validator": "^13.7.2",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
